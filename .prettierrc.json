{
  "arrowParens": "always",
  "bracketSpacing": true,
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "pugSingleQuote": false,
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": false,
  "pugSortAttributesBeginning": [
    "^cols$",
    "^v-for$",
    "^:key$",
    "^v-if$",
    "^v-else-if$",
    "^v-else$",
    "^v-on$",
    "^v-bind$",
    "^ref$",
    "^v-model",
    "^name$",
    "^:?type$",
    "^:value$",
    "^v-text$",
    "^:?label$",
    "^:headers$",
    "^:items$",
    "^:?item-key$",
    "^:?item-text$",
    "^:?item-value$",
    "^:?item-disabled$",
    "^:?must-sort$",
    "^:?sort-by$",
    "^:?sort-desc$",
    "^:?placeholder$",
    "^:?src$",
    "^:?app$",
    "^:?color$",
    "^:?text-color$",
    "^:?background-color$",
    "^:?dark$",
    "^:?icon$",
    "^:?align$",
    "^:?justify$",
    "^:?dense$",
    "^:?small$",
    "^:?outlined$"
  ],
  "pugSortAttributesEnd": [
    "^:?hint$",
    "^:?persistent-hint$",
    "^prepend-",
    "^@click:prepend",
    "^append-",
    "^@click:append",
    "^:to$",
    "^exact$",
    "^:(?!(width|height|loading|disabled|data-))",
    "^target$",
    "^:?width$",
    "^:?height$",
    "^@click",
    "^@",
    "^v-shortkey",
    "^@shortkey",
    "^:loading$",
    "^:disabled$",
    "^:?data-"
  ]
}
